# Wait for the CloudBeaver to start
echo "$(date): Waiting for CloudBeaver to open port ${port}..."

if wait_until_port_used "${host}:${port}" 600; then
  echo "$(date): Discovered CloudBeaver listening on port ${port}!"
else
  echo "$(date): Timed out waiting for CloudBeaver to open port ${port}!"
  clean_up 1
fi

sleep 2
